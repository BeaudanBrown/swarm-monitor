  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "console": "integratedTerminal",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch command",
      "program": "${workspaceFolder}/index.ts",
      "args": ["command"],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "console": "integratedTerminal",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch snode_stats",
      "program": "${workspaceFolder}/index.ts",
      "args": ["snode_stats"],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "console": "integratedTerminal",
    },
  ]
}
