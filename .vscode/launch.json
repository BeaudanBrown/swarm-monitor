  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch get_stats",
      "program": "${workspaceFolder}/index.ts",
      "args": ["get_stats"],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch poll_stats",
      "program": "${workspaceFolder}/index.ts",
      "args": ["poll_stats"],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
  ]
}
